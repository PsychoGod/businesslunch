{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://staging-businesslunch.herokuapp.com/",
  "resourcePath": "tokens",
  "apis": [
    {
      "path": "api/token.json",
      "operations": [
        {
          "summary": "Exchange credentials to token",
          "parameters": [
            {
              "paramType": "form",
              "name": "grant_type",
              "type": "string",
              "description": "Grant Type",
              "required": true,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "password"
                ]
              }
            },
            {
              "paramType": "form",
              "name": "email",
              "type": "string",
              "description": "Email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "string",
              "description": "Password",
              "required": true
            },
            {
              "paramType": "form",
              "name": "client_id",
              "type": "string",
              "description": "Application ID",
              "required": true
            },
            {
              "paramType": "form",
              "name": "client_secret",
              "type": "string",
              "description": "Application Secret Key",
              "required": true
            },
            {
              "paramType": "form",
              "name": "parse_object_id",
              "type": "string",
              "description": "Parse Installation Object ID",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Success"
            },
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            }
          ],
          "method": "post",
          "nickname": "Api::Tokens#create"
        }
      ]
    }
  ]
}